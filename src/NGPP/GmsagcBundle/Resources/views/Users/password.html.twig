{% extends "::base.html.twig" %}

{% block content %}
    <form action="{{app.request.requestUri}}" method="post" {{ form_enctype(form) }}>
        <fieldset>
          <legend>{{ 'users.save'|trans }}</legend>
            {{ form_widget(form) }}
            <br>
            <input type="submit" class="btn btn-success" value="{{ 'action.save'|trans }}"/>
            <a href="{{ path('ngpp_gmsagc_users') }}" class="btn">
                {{ 'action.cancel'|trans }}
            </a>
        </fieldset>
    </form>
{% endblock %}
